<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="match_parent"
	android:layout_width="match_parent">
	<TableLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
  		android:layout_width="match_parent"
  		android:layout_height="match_parent"
  		android:stretchColumns="1">
  	
  		<TableRow>
  			<TextView
        		android:text="Version"
        		android:padding="3dip" />
        	<EditText
	        	android:enabled="false"
    	    	android:id="@+id/textEntryModelVersion"
        		android:padding="3dip"
        		android:layout_width="fill_parent" android:layout_marginRight="3dip"/>
	  	</TableRow>
  	
	  	<TableRow>
  			<TextView
	        	android:text="Created On"
    	    	android:padding="3dip" />
        	<EditText
        		android:enabled="false"
	        	android:id="@+id/textEntryModelCreationDate"
    	    	android:padding="3dip"
        		android:layout_width="fill_parent" 
        		android:layout_marginRight="5dip"/>
  		</TableRow>
  		<TableRow>
	  		<TextView
    	    	android:text="Updated On"
        		android:padding="3dip" />
        	<EditText
        		android:enabled="false"
        		android:id="@+id/textEntryModelModificationDate"
	        	android:padding="3dip"
    	    	android:layout_width="fill_parent" 
    	    	android:layout_marginRight="5dip"/>
  		</TableRow>
  		<TableRow>
	  		<TextView
    	    	android:text="Expires On"
	        	android:padding="3dip" />
    	    <EditText
        		android:enabled="false"
	        	android:id="@+id/textEntryModelExpirationDate"
        		android:padding="3dip"
        		android:layout_width="fill_parent" 
        		android:layout_marginRight="5dip"/>
  		</TableRow>
  		
  		<TableRow>
  			<TextView
	        	android:text="Schedule"
    	    	android:padding="3dip" />
        	<Spinner
	        	android:id="@+id/spinnerModelSchedule"
    	    	android:padding="3dip"
        		android:layout_width="fill_parent" 
				android:layout_marginRight="5dip"/>
  		</TableRow>
  		<TableRow>
	  		<TextView
    	    	android:text="Agg Fx"
	        	android:padding="3dip" />
	        <Spinner
	        	android:id="@+id/spinnerModelAggFx"
    	    	android:padding="3dip"
        		android:layout_width="fill_parent" 
				android:layout_marginRight="5dip"/>
  		</TableRow>
  		
  		<TableRow>
	  		<TextView
    	    	android:text="Window Size"
	        	android:padding="3dip" />
    	    <EditText
	        	android:id="@+id/textEntryModelWindowSize"
        		android:padding="3dip"
        		android:layout_width="fill_parent"
        		android:layout_marginRight="5dip"/>
  		</TableRow>   
  		<TableRow>
	  		<TextView
    	    	android:text="Granularity"
	        	android:padding="3dip" />
    	    <EditText
	        	android:id="@+id/textEntryModelGranularity"
        		android:padding="3dip"
        		android:layout_width="fill_parent" 
        		android:layout_marginRight="5dip"/>
  		</TableRow>
  		
  		<TableRow>
	  		<TextView
    	    	android:text="Freshness"
	        	android:padding="3dip" />
    	    <EditText
	        	android:id="@+id/textEntryModelFreshness"
        		android:padding="3dip"
        		android:layout_width="fill_parent" 
        		android:layout_marginRight="5dip"/>
  		</TableRow>
  		<TableRow>
	  		<TextView
    	    	android:text="Earliest Rebuild"
	        	android:padding="3dip" />
    	    <EditText
	        	android:id="@+id/textEntryModelEarliestRebuild"
        		android:padding="3dip"
        		android:layout_width="fill_parent" 
        		android:layout_marginRight="5dip"/>
  		</TableRow>  
	</TableLayout>
</ScrollView>